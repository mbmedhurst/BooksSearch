{
  "name": "bookssearch",
  "version": "1.0.0",
  "description": "![Books Search Demo](client/src/assets/images/BookSearchDemo.gif)",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.20.1",
    "build": "^0.1.4",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.3"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbmedhurst/BooksSearch.git"
  },
